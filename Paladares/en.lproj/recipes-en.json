      "name": "X-Caboquinho",
      "image": "caboquinho",
      "imageList": "",
      "country": "Brazil",
      "people": "4 people",
      "time": "30 minutes",
      "curiosity": "The X-Caboquinho is a traditional dish from Manaus, known as one of the most iconic snacks in Amazonian culture. It combines tucumã, an Amazonian fruit, with grilled cheese and bread.",
      "utensils": [
        "Frying pan",
        "Chef's knife",
        "Spatula"
      ],
      "ingredients": [
        { "quantity": "4", "unit": "French rolls", "name": "French rolls" },
        { "quantity": "12", "unit": "slices", "name": "coalho cheese" },
        { "quantity": "4", "unit": "tablespoons", "name": "butter" },
        { "quantity": "4", "unit": "cups", "name": "tucumã strips" },
        { "quantity": "4", "unit": "bananas", "name": "bananas" }
      ],
      "suggestions": [
        "Serve with cassava flour on the side."
      ],
      "preparationMethods": [
        {
          "chefId": 1,
          "stage": "Preparation of the Bread",
          "steps": [
            {
              "stepNumber": "Step 1",
              "description": "Cut the French rolls in half, but do not separate the halves completely."
            },
            {
              "stepNumber": "Step 2",
              "description": "Spread butter on the inner halves of each roll."
            },
            {
              "stepNumber": "Step 3",
              "description": "Lightly heat the rolls in a frying pan to brown the butter."
            }
          ]
        },
        {
          "chefId": 2,
          "stage": "Preparation of the Tucumã",
          "steps": [
            {
              "stepNumber": "Step 1",
              "description": "Cut the tucumã into thin, even strips."
            },
            {
              "stepNumber": "Step 2",
              "description": "Place the tucumã strips in a bowl and set aside."
            }
          ]
        },
        {
          "chefId": 3,
          "stage": "Preparation of the Coalho Cheese",
          "steps": [
            {
              "stepNumber": "Step 1",
              "description": "Cut the coalho cheese into slices and grill until golden on both sides."
            },
            {
              "stepNumber": "Step 2",
              "description": "Place the grilled cheese on a plate and set aside."
            }
          ]
        },
        {
          "chefId": 4,
          "stage": "Preparation of the Bananas and Assembly",
          "steps": [
            {
              "stepNumber": "Step 1",
              "description": "Peel the bananas and cut them into thin slices."
            },
            {
              "stepNumber": "Step 2",
              "description": "In a frying pan, heat the banana slices until they are lightly golden."
            },
            {
              "stepNumber": "Step 3",
              "description": "Assemble the sandwiches by adding the ingredients prepared by the other chefs: tucumã strips, grilled cheese, and banana slices."
            },
            {
              "stepNumber": "Step 4",
              "description": "Place the assembled sandwiches back in the hot frying pan and cook for 2 minutes on each side, making sure all the ingredients are well integrated."
            }
          ]
        }
      ]
    },
    
